		<pair id="s2.p3" e1="s2.e2"
		    e2="s2.e4" pgr="true"/>
	</sentence>
	<sentence id="s3" text="However, when SDHB-tumors give rise to metastases, metastatic cells are able to thrive with decreased probability of regression compared with SDHD counterparts (p = 0.17 vs 0.89).">
		<entity id="s3.e1" charOffset="14-18"
			type="GENE" text="SDHB" ontology_id="6390"/>
		<entity id="s3.e2" charOffset="19-24"
