	<sentence id="s1" text="Nuclear respiratory factor 1 (NRF1) transcription factor has recently been shown to control breast cancer progression.">
		<entity id="s1.e1" charOffset="30-34"
			type="GENE" text="NRF1" ontology_id="4779"/>
