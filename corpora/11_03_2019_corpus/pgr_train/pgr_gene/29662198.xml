<document id="29662198">
	<sentence id="s1" text="Tumor-initiating cells (TIC) represent a subset of tumor cells with increased self-renewal capability.">
		<entity id="s1.e1" charOffset="24-27"
			type="GENE" text="TIC" ontology_id="23550"/>
