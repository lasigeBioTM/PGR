		<pair id="s3.p1" e1="s3.e1"
		    e2="s3.e2" pgr="false"/>
	</sentence>
	<sentence id="s4" text="Here we identify a novel mechanism of NOTCH regulation and TIC induction in breast cancer via the miR-106b-25 miRNA cluster.">
		<entity id="s4.e1" charOffset="59-62"
			type="GENE" text="TIC" ontology_id="23550"/>
		<entity id="s4.e2" charOffset="83-89"
			type="HP" text="cancer" ontology_id="HP_0002664"/>
