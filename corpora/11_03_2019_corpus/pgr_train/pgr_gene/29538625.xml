	<sentence id="s2" text="We characterized the cellular deficits for an allelic series of seven STXBP1 mutations and developed four mouse models that recapitulate the abnormal EEG activity and cognitive aspects of human STXBP1-encephalopathy.">
		<entity id="s2.e1" charOffset="70-76"
			type="GENE" text="STXBP1" ontology_id="6812"/>
