	<sentence id="s4" text="Furthermore, the methylation status of ADAMTS18 was not significantly associated with gender, age, location, tumor diameter, pathological stage, nuclear grade or short-term DFS in patients with ccRCC (p AAAA 0.05).">
		<entity id="s4.e1" charOffset="39-47"
			type="GENE" text="ADAMTS18" ontology_id="170692"/>
