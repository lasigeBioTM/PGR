	<sentence id="s4" text="Additionally, inhibition of Chmp1A expression by shRNA induced tumor formation in normal renal cells.">
		<entity id="s4.e1" charOffset="28-34"
			type="GENE" text="Chmp1A" ontology_id="5119"/>
