		<pair id="s3.p1" e1="s3.e1"
		    e2="s3.e2" pgr="false"/>
	</sentence>
	<sentence id="s4" text="It is autosomal recessively inherited, and caused by SPINT2 mutations.">
		<entity id="s4.e1" charOffset="6-25"
			type="HP" text="autosomal recessive" ontology_id="HP_0000007"/>
		<entity id="s4.e2" charOffset="53-59"
