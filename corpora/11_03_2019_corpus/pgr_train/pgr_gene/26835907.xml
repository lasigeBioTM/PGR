	<sentence id="s2" text="CSD is clinically and genetically heterogeneous.">
		<entity id="s2.e1" charOffset="0-3"
			type="GENE" text="CSD" ontology_id="51380"/>
		<entity id="s2.e2" charOffset="34-47"
