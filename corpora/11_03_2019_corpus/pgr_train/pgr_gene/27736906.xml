	<sentence id="s1" text="HIV-1 transgenic mice on the FVB/NJ background (TgFVB) represent a validated model of HIV-associated nephropathy (HIVAN).">
		<entity id="s1.e1" charOffset="17-21"
			type="GENE" text="mice" ontology_id="23581"/>
