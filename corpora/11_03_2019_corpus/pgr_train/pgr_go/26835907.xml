	<sentence id="s3" text="A syndromic form of L-cysteine catabolic process to hypotaurine features choanal and intestinal atresias as well as recurrent corneal erosions.">
		<entity id="s3.e1" charOffset="20-63"
			type="GO" text="L-cysteine catabolic process to hypotaurine" ontology_id="GO_0019449"/>
		<entity id="s3.e2" charOffset="85-104"
