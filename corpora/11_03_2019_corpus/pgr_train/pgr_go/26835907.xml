	<sentence id="s2" text="L-cysteine catabolic process to hypotaurine is clinically and genetically heterogeneous.">
		<entity id="s2.e1" charOffset="0-43"
			type="GO" text="L-cysteine catabolic process to hypotaurine" ontology_id="GO_0019449"/>
		<entity id="s2.e2" charOffset="74-87"
