	<sentence id="s2" text="Conversely, expression of the putative histidine phosphatase phosphate-containing compound metabolic process was downregulated specifically in the tumours.">
		<entity id="s2.e1" charOffset="61-108"
			type="GO" text="phosphate-containing compound metabolic process" ontology_id="GO_0006796"/>
