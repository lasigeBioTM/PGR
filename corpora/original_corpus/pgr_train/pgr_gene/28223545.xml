	<sentence id="s2" text="In the present study, we found that RNF6 was overexpressed in more than 70% of breast cancer tissues and it was associated with overall survival.">
		<entity id="s2.e1" charOffset="36-40"
			type="GENE" text="RNF6" ontology_id="6049"/>
