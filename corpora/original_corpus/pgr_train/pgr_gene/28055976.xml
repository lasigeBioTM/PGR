	<sentence id="s2" text="In this study, we found that CD133 mRNA expression was a prognostic marker in three independent glioma patient cohorts, corroborating a putative role for CD133 in glioma progression.">
		<entity id="s2.e1" charOffset="29-34"
			type="GENE" text="CD133" ontology_id="8842"/>
