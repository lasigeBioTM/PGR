		<pair id="s7.p1" e1="s7.e1"
		    e2="s7.e2" pgr="false"/>
	</sentence>
	<sentence id="s8" text="Further mechanistic studies showed that ADAMTS18 suppressed epithelial-mesenchymal transition (EMT), further inhibited migration and invasion of breast cancer cells.">
		<entity id="s8.e1" charOffset="40-48"
			type="GENE" text="ADAMTS18" ontology_id="170692"/>
