		<pair id="s4.p2" e1="s4.e1"
		    e2="s4.e2" pgr="false"/>
	</sentence>
	<sentence id="s5" text="ADAMTS18 promoter methylation was detected in 70.8% of tumor tissues by methylation-specific PCR, but none of the normal tissues.">
		<entity id="s5.e1" charOffset="0-8"
			type="GENE" text="ADAMTS18" ontology_id="170692"/>
		<entity id="s5.e2" charOffset="55-60"
