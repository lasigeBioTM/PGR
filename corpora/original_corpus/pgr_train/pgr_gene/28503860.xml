		<pair id="s2.p1" e1="s2.e1"
		    e2="s2.e2" pgr="false"/>
	</sentence>
	<sentence id="s3" text="Here, we found that ADAMTS18 was silenced or downregulated in breast cancer cell lines.">
		<entity id="s3.e1" charOffset="20-28"
			type="GENE" text="ADAMTS18" ontology_id="170692"/>
		<entity id="s3.e2" charOffset="69-75"
