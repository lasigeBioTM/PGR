		<pair id="s3.p1" e1="s3.e1"
		    e2="s3.e2" pgr="false"/>
	</sentence>
	<sentence id="s4" text="ADAMTS18 was reduced in primary breast tumor tissues as compared with their adjacent noncancer tissues.">
		<entity id="s4.e1" charOffset="0-8"
			type="GENE" text="ADAMTS18" ontology_id="170692"/>
		<entity id="s4.e2" charOffset="39-44"
