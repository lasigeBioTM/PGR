		<pair id="s6.p1" e1="s6.e1"
		    e2="s6.e2" pgr="false"/>
	</sentence>
	<sentence id="s7" text="Nude mouse model further confirmed that ADAMTS18 suppressed breast cancer metastasis in vivo.">
		<entity id="s7.e1" charOffset="40-48"
			type="GENE" text="ADAMTS18" ontology_id="170692"/>
		<entity id="s7.e2" charOffset="67-73"
