	<sentence id="s3" text="Here, we used immunohistochemical staining to measure CCDC8 expression in 147 samples from tumors and 30 samples from the adjacent normal lung tissues of patients with non-small cell lung cancer.">
		<entity id="s3.e1" charOffset="54-59"
			type="GENE" text="CCDC8" ontology_id="83987"/>
