	<sentence id="s3" text="The mRNA and protein levels of STX3 are significantly up-regulated in human breast cancer compared with matched adjacent non-cancer tissues.">
		<entity id="s3.e1" charOffset="31-35"
			type="GENE" text="STX3" ontology_id="6809"/>
		<entity id="s3.e2" charOffset="83-89"
