		<pair id="s3.p1" e1="s3.e1"
		    e2="s3.e2" pgr="false"/>
	</sentence>
	<sentence id="s4" text="The up-regulation of STX3 is correlated with high disease stage and predicts overall and disease-free survival in patients with breast cancer.">
		<entity id="s4.e1" charOffset="21-25"
			type="GENE" text="STX3" ontology_id="6809"/>
		<entity id="s4.e2" charOffset="135-141"
