	<sentence id="s2" text="Here we report that STX3 acts as an oncogenic protein in human breast cancer.">
		<entity id="s2.e1" charOffset="20-24"
			type="GENE" text="STX3" ontology_id="6809"/>
		<entity id="s2.e2" charOffset="70-76"
