	<sentence id="s5" text="We show that HOXB1 expression is significantly downregulated in glioma tissues and cell lines, and that its expression may be closely associated with the degree of malignancy.">
		<entity id="s5.e1" charOffset="13-18"
			type="GENE" text="HOXB1" ontology_id="3211"/>
		<entity id="s5.e2" charOffset="64-70"
